%h2 Log in
= form_with url: sessions_path, local: true do |f|
  .field
    = f.label :email
    %br/
    = f.email_field :email, autofocus: true
  .field
    = f.label :password
    %br/
    = f.password_field :password, autocomplete: "off"
  .field
    = f.check_box :remember_me
    = f.label :remember_me
  .actions
    = f.submit "Log in"
